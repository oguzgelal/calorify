<div class="container">

  <div class="row">

    <div class="col-sm-6 col-sm-offset-3 col-xs-12">
      <h3>RDF Parser</h3>
    </div>

    <div class="col-sm-6 col-sm-offset-3 col-xs-12" style="margin-top: 20px;">
      <form action="/rdf-upload" enctype="multipart/form-data" method="post">
        <div class="form-group">
          <label for="fileInput">File input</label>
          <input type="file" name="rdf" id="fileInput">
          <p class="help-block">Chose the RDF/XML file you wish to parse</p>
        </div>
        <button type="submit" class="btn btn-default" id="parseButton">Parse</button>
      </form>
    </div>

  </div>

</div>
